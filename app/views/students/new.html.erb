<h1>Student Form</h1>

<form method="post" action="/students">

<label>First Name:</label><br>
  <input type="text" id="student[first_name]" name="first_name">
  <label>Last Name:</label><br>
  <input type="text" id="student[last_name]" name="last_name">
  <%= submit_tag "Submit Student" %>
</form>
